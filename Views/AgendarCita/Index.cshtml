
@{
    ViewData["Title"] = "Agendar Citas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container">
    <div class="container container-fluid">
        <div class="titulo">
        
            <h1>Agendar Citas</h1>
   
        </div>


        <form>
            <div class="form-group row">
                <label for="inputCedula1" class="col-sm-1 col-form-label">Cedula</label>
                <div class="col-sm-5">
                    <input type="text" class="form-control" id="inputCedula" placeholder="Cedula">
                </div>
            </div>
            <div class="form-group row">
                <label for="inputNombre1" class="col-sm-1 col-form-label">Nombre</label>
                <div class="col-sm-5">
                    <input type="text" class="form-control" id="inputNombre" placeholder="Nombre">
                </div>
            </div>

            <div class="form-group row">
                <label for="inputApellido1" class="col-sm-1 col-form-label">Apellido</label>
                <div class="col-sm-5">
                    <input type="text" class="form-control" id="inputApellido" placeholder="Apellido">
                </div>
            </div>

            <div class="form-group row">
                <label for="inputTelefono1" class="col-sm-1 col-form-label">Telefono</label>
                <div class="col-sm-5">
                    <input type="text" class="form-control" id="inputTelefono" placeholder="Telefono">
                </div>
            </div>


            <div class="form-group row">
                <div class="col-sm-10">
                    <button type="submit" class="btn btn-primary">Registrar Paciente</button>
                </div>
            </div>
        </form>
    </div>
    
</div>


